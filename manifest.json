{
  "manifest_version": 2,

  "name": "Gif This",
  "description": "Searches for a gif and pastes the link on an input box",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "gifThis.html"
  },
  "permissions":[
    "clipboardWrite",
    "storage"
  ],

  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon.png",
    "128": "icon128.png"
  }
}
